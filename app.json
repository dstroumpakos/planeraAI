{
  "expo": {
    "name": "Planera AI Travel Planner",
    "slug": "planera-ai-travel-planner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appicon-1024x1024-01-9h6cls.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAF9F6"
    },
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "bundleIdentifier": "com.dstroump.planeraaitravelplanner",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#FAF9F6"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appicon-1024x1024-01-9h6cls.jpg",
        "backgroundColor": "#FAF9F6"
      },
      "newArchEnabled": true,
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#FAF9F6"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FAF9F6"
        }
      ],
      "expo-secure-store",
      "expo-font",
      "expo-web-browser",
      "@react-native-google-signin/google-signin",
      "expo-iap"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2ac0a8a4-c019-48ac-b8a3-4651a864b089"
      }
    }
  }
}
